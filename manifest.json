{
  "name": "pixivユーザー検索 for ニコニコ静画",
  "version": "1.2.1",
  "manifest_version": 2,
  "description": "ニコニコ静画の投稿者をpixivで検索する拡張機能",
  "icons" : {
    "16" : "icon/icon16.png",
    "32" : "icon/icon32.png",
    "48" : "icon/icon48.png",
    "128" : "icon/icon128.png"
  },
  "content_scripts": [
    {
      "js": ["js/script.js"],
      "css": ["css/script.css"],
      "matches": ["https://seiga.nicovideo.jp/seiga/*"]
    }
  ],
  "web_accessible_resources": ["html/searchMenu.html"]
}
